{
  "name": "ts-next-eslint-prettier",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "develop": "run-p develop:*",
    "develop:next": "next dev",
    "build": "next build",
    "preview": "next start",
    "lint": "run-p lint:*",
    "lint:next": "next lint",
    "lint:style": "stylelint src/**/*.{css,scss,sass} --allow-empty-input",
    "typecheck": "tsc --project tsconfig.json --noEmit --skipLibCheck"
  },
  "dependencies": {
    "next": "^13.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.16.3",
    "@types/react": "18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.3.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^5.0.4"
  }
